{
  "name": "aimpr",
  "version": "0.0.0",
  "homepage": "https://github.com/milushov/aimpr",
  "authors": [
    "Roma Milushov <roma@milushov.ru>"
  ],
  "description": "angular app for improving music on vk",
  "main": "build/index.html",
  "keywords": [
    "angular",
    "vk"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "~1.3.0",
    "angular-cookies": "~1.3.0",
    "angular-animate": "~1.3.0",
    "angular-route": "~1.3.0",
    "q": "~1.0.1",
    "bootstrap-css-only": "~3.3.0",
    "angular-truncate": "*",
    "ngstorage": "~0.3.0",
    "ngInfiniteScroll": "~1.1.2",
    "jquery": "~2.1.1",
    "angular-notify": "~2.0.2",
    "async": "~0.9.0",
    "async-each": "~0.1.6",
    "ladda": "~0.9.4",
    "angular-elastic": "~2.4.2",
    "bootswatch-sass": "~3.3.1"
  },
  "devDependencies": {
    "angular-mocks": "~1.2.0",
    "angular-scenario": "~1.2.0"
  },
  "appPath": "app",
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/**/*.js",
        "dist/**/*.css"
      ]
    },
    "ladda": {
      "main": [
        "dist/**/*.js",
        "dist/**/*.css"
      ]
    },
    "bootswatch-sass": {
      "main": [
        "flatly/*.js",
        "flatly/*.css"
      ]
    }
  }
}
